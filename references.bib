journal = {American Journal of Orthodontics and Dentofacial Orthopedics},
number = {4},
pages = {448--455},
pmid = {11029742},
title = {{The Royal London Space Planning: An integration of space analysis and treatment planning Part I: Assessing the space required to meet treatment objectives}},
