title = {{Support vector machine classification and validation of cancer tissue samples using microarray expression data}},
url = {http://bioinformatics.oxfordjournals.org/content/16/10/906{\%}5Cnhttp://bioinformatics.oxfordjournals.org/cgi/doi/10.1093/bioinformatics/16.10.906},
