doi = {10.1109/IMSCCS.2007.52},
file = {:C$\backslash$:/Users/polla/Google Drive/PhD/Papers/Mendeley/Yingqiu, Wei, Yunchun - 2007 - Network Traffic Classification Using K-means Clustering.pdf:pdf},
