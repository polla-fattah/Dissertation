url = {https://books.google.ca/books/about/An{\_}Introduction{\_}to{\_}Genetic{\_}Algorithms.html?id=0eznlz0TF-IC{\&}pgis=1},
year = {1998}
}
@misc{Itaya2010,
abstract = {Public goods are often publicly provided but they may also be privately provided. Modelling private provision as the Nash equilibrium of a contribution game predicts that the equilibrium will be inefficient. In addition, the model also provides a number of further predictions: income redistribution will not affect the level of provision, the mean contribution per-person will be zero in a large economy and public provision leads to one-to-one crowding out. Furthermore, if all individuals contribute towards the provision of a public good their utilities are equalised even if the income distribution is unequal. It may then seem that there is no role for redistributive policy, but social welfare can be raised by creating sufficient income inequality so that only the rich provide public goods. The design of income and commodity taxes is highly influenced by this tendency for redistribution. The paper concludes by reviewing evidence on the model's basic predictions and discussing possible extensions.},
author = {Itaya, Jun-ichi and de Meza, David and Myles, Gareth D.},
