journal = {Proceedings of the ninth ACM SIGKDD international conference on Knowledge discovery and data mining - KDD '03},
keywords = {clustering,distance measure,distances,dynamic time warping,handwriting,time series,toread},
pages = {216},
title = {{Indexing multi-dimensional time-series with support for multiple distance measures}},
url = {http://citeseer.ist.psu.edu/viewdoc/summary?doi=10.1.1.136.1249{\%}5Cnhttp://portal.acm.org/citation.cfm?doid=956750.956777},
